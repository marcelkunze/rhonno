# Training of tracks with NNO

#define the network topology
xmlp 6 20 10 1
transfer TR_FERMI
momentum 0.2
balance true
plots true
test 10000
start 1
stop 100

#define the data source
datapath /Users/marcel/workspace/rhonno/Data
networkpath ../Networks
file tracks.root

#set up the input layer (use branch names)
tree tracks
input rz1:phi1:z1:rz2:phi2:z2
#autoscale true
#cut p<2.0

#set up the output layer (use branch names)
output truth==1

#end of file
		
